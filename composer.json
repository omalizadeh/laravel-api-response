{
    "name": "omalizadeh/laravel-api-response",
    "description": "Basic and standard api response format for laravel using json resources",
    "homepage": "https://github.com/omalizadeh/laravel-api-response",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "request",
        "resource",
        "laravel",
        "package",
        "response",
        "json",
        "collection"
    ],
    "authors": [
        {
            "name": "Omid Alizadeh",
            "email": "om_alizadeh@yahoo.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laravel/framework": "^8.67 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Omalizadeh\\ApiResponse\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "require-dev": {
        "orchestra/testbench": "^6.24 || ^7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Omalizadeh\\ApiResponse\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omalizadeh\\ApiResponse\\ApiResponseServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
